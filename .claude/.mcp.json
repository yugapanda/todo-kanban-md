{
    "mcpServers": {
        "mcp-server-time": {
            "command": "python",
            "args": [
                "-m",
                "mcp_server_time",
                "--local-timezone=America/Los_Angeles"
            ],
            "env": {}
        },
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ]
        }
    }
}